<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="300" android:exitFadeDuration="300">
<item android:state_pressed="false">
    <shape>
        <gradient android:angle="180" android:endColor="#CBE49D43" android:startColor="#CBBE8A47" />
        <corners android:radius="20dp" />
    </shape>
</item>
<item android:state_pressed="true">
    <shape>
        <gradient android:angle="0" android:endColor="#283033" android:startColor="#322942" />
        <corners android:radius="20dp" />
    </shape>
</item>
</selector>