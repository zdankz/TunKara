<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="300" android:exitFadeDuration="300">
    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="180" android:endColor="#D5BBB5" android:startColor="#A7865A" />
            <corners android:radius="145dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="0" android:endColor="#ADD69C" android:startColor="#96CCD8" />
            <corners android:radius="140dp" />
        </shape>
    </item>
</selector>